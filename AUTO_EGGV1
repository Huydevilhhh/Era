local itemName = "Paradise Egg x1" -- T√™n ch√≠nh x√°c c·ªßa m√≥n ƒë·ªì

local player = game.Players.LocalPlayer
local backpack = player:WaitForChild("Backpack")
local function getCharacter()
    return player.Character or player.CharacterAdded:Wait()
end

-- H√†m ƒë·ªÉ c·∫ßm m√≥n ƒë·ªì
local function equipItem()
    local character = getCharacter()
    local tool = backpack:FindFirstChild(itemName)
    if tool then
        tool.Parent = character
        print("ƒê√£ c·∫ßm m√≥n ƒë·ªì:", itemName)
    else
        warn("Kh√¥ng t√¨m th·∫•y m√≥n ƒë·ªì:", itemName)
    end
end

-- G·ªçi h√†m khi load nh√¢n v·∫≠t ho·∫∑c sau v√†i gi√¢y
player.CharacterAdded:Connect(function()
    wait(1)
    equipItem()
end)

-- N·∫øu nh√¢n v·∫≠t ƒë√£ c√≥ s·∫µn th√¨ g·ªçi lu√¥n
if player.Character then
    wait(1)
    equipItem()
end
wait(0.3)
local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Danh s√°ch c√°c t·ª´ kho√° GUI c√≥ th·ªÉ l√† m√†n h√¨nh ch·ªù
local blacklist = { "Loading", "Intro", "Welcome", "StartScreen", "Tutorial", "MainUI" }

-- H√†m ki·ªÉm tra v√† xo√° c√°c GUI c√≥ t√™n gi·ªëng t·ª´ kho√°
local function removeIntroGUIs()
    for _, gui in pairs(playerGui:GetChildren()) do
        for _, keyword in pairs(blacklist) do
            if gui.Name:lower():find(keyword:lower()) then
                gui:Destroy()
                print("üö´ ƒê√£ xo√° GUI:", gui.Name)
            end
        end
    end
end

-- G·ªçi l√∫c ƒë·∫ßu
removeIntroGUIs()

-- G·ªçi khi c√≥ GUI m·ªõi ƒë∆∞·ª£c t·∫°o
playerGui.ChildAdded:Connect(function(child)
    wait(0.1)
    removeIntroGUIs()
end)
Wait()
local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- Ch·ªù nh√¢n v·∫≠t xu·∫•t hi·ªán
local function resetCamera()
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:WaitForChild("Humanoid")
    local camera = workspace.CurrentCamera

    -- ƒê·∫∑t camera v·ªÅ ch·∫ø ƒë·ªô m·∫∑c ƒë·ªãnh theo nh√¢n v·∫≠t
    camera.CameraSubject = humanoid
    camera.CameraType = Enum.CameraType.Custom

    print("üé• Camera ƒë√£ ƒë∆∞·ª£c reset v·ªÅ m·∫∑c ƒë·ªãnh.")
end

-- G·ªçi sau khi v√†o game ho·∫∑c sau khi tho√°t GUI
task.delay(1, resetCamera)

-- G·ªçi l·∫°i m·ªói l·∫ßn nh√¢n v·∫≠t load m·ªõi
player.CharacterAdded:Connect(function()
    task.wait(1)
    resetCamera()
end)
wait(0.7)
local Players = game:GetService("Players")
local VirtualInput = game:GetService("VirtualInputManager")
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")

-- H√†m click d∆∞·ªõi ch√¢n
local function clickBelowFeet(times)
	local camera = workspace.CurrentCamera
	local footPos = hrp.Position - Vector3.new(0, 2, 0)
	local screenPos, onScreen = camera:WorldToViewportPoint(footPos)

	if onScreen then
		for i = 1, times do
			VirtualInput:SendMouseButtonEvent(screenPos.X, screenPos.Y, 0, true, game, 0)
			VirtualInput:SendMouseButtonEvent(screenPos.X, screenPos.Y, 0, false, game, 0)
			task.wait(0.1)
		end
	else
		warn("‚ö†Ô∏è Kh√¥ng th·ªÉ click d∆∞·ªõi ch√¢n: v·ªã tr√≠ ngo√†i m√†n h√¨nh.")
	end
end

-- B·∫Øt ƒë·∫ßu h√†nh ƒë·ªông
local forward = hrp.CFrame.LookVector
local left = -hrp.CFrame.RightVector
local right = hrp.CFrame.RightVector

-- üö∂ Ti·∫øn th·∫≥ng 30 studs
hrp.CFrame = hrp.CFrame + (forward * 30)
task.wait(0.2)

-- ‚Ü©Ô∏è Qu·∫πo tr√°i 22 studs
hrp.CFrame = hrp.CFrame + (left * 22)
task.wait(0.3)

-- üñ±Ô∏è Click 10 l·∫ßn d∆∞·ªõi ch√¢n
clickBelowFeet(10)
task.wait(0.3)

-- üö∂ Ti·∫øn ti·∫øp 7 studs (c·∫≠p nh·∫≠t forward sau qu·∫πo)
forward = hrp.CFrame.LookVector
hrp.CFrame = hrp.CFrame + (forward * 7)
task.wait(0.3)

-- üñ±Ô∏è Click 10 l·∫ßn
clickBelowFeet(10)
task.wait(0.3)

-- ‚Ü™Ô∏è Qu·∫πo ph·∫£i 7 studs (c·∫≠p nh·∫≠t right theo h∆∞·ªõng m·ªõi)
right = hrp.CFrame.RightVector
hrp.CFrame = hrp.CFrame + (right * 7)
task.wait(0.3)

-- üñ±Ô∏è Click 10 l·∫ßn
clickBelowFeet(10)

-- üèÖ Hi·ªán badge "Complete the mission"
local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
gui.Name = "CompletionBadge"
gui.ResetOnSpawn = false

local label = Instance.new("TextLabel", gui)
label.Size = UDim2.new(0, 240, 0, 40)
label.Position = UDim2.new(1, -250, 1, -60)
label.BackgroundColor3 = Color3.fromRGB(30, 180, 80)
label.TextColor3 = Color3.new(1, 1, 1)
label.Font = Enum.Font.SourceSansBold
label.TextSize = 20
label.Text = "‚úÖ Complete the mission"
label.BackgroundTransparency = 0.1
label.BorderSizePixel = 0
label.TextStrokeTransparency = 0.5

-- üïê T·ª± ƒë·ªông xo√° sau 5 gi√¢y
task.delay(5, function()
	gui:Destroy()
end)

print("‚úÖ Nhi·ªám v·ª• ho√†n th√†nh.")
