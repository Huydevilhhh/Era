local args = {
	{
		Type = "Code",
		Mode = "Redeem",
		Code = "ONEEIGHTYFIVELIKES"
	}
}
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("GetFunction"):InvokeServer(unpack(args))
wait(0.5)
